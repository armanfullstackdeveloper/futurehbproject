@model IEnumerable<DataModel.Models.ViewModel.ShoppingBagViewModel>


@{
    ViewBag.Title = "سبد خرید";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@section Style{

}

<div class="container" ng-controller="orderCtrl">

    <div ng-show="!Data || Data.length==0" style="text-align: center">
        <div><img src="~/Img/emptyBascket.jpg" /></div>
        <div>
            <div> <b>سبد خرید شما خالی است</b></div>
            <div>
                برای خرید انواع البسه و سایر لوازم مورد نیاز خود ابتدا وارد شوید و از امکانات کامل ما
                استفاده نمایید. شما میتوانید با استفاده از دکمه سبد خرید محصول مورد نظرتان را به
                سبدتان اضافه نمایید و از همینجا اقدام به خرید نمایید
            </div>
        </div>
    </div>
    <div class="sizeManager" ng-show="Data && Data.length!= 0">
        <div class="storeCheck relativeMatter organizer">
            <!-- dar soorati k mahsool takhfif nadashte bashe -->
            <div class="storePricesManagement organizer standardVerticalDoubleBottomPadding">
                <div class="fourSixColumn">
                    <div class="storeBox reallyLightBack organizer boxElement radiusBox relativeMatter noOverFlows">
                        <div class="storeName borderBottom loginPadding eightyFontsize thickFont almostGrayColor organizer">
                            <div class="profilePic goRight relativeMatter standardHorizontalLeftMargin">
                                <img src="" class="imageInMiddle">
                                <div class="openUpObject profileMiniBox textInCenter loginPadding boxElement radiusBox relativeMatter">
                                    <div class="topPointer organizer"></div>
                                    <div class="profilePic relativeMatter blockInlineMode">
                                        <img src="" class="imageInMiddle">
                                    </div>
                                    <div class="organizer pinkColor">نام فروشگاه</div>

                                    <div class="organizer standardPadding">
                                        <div class="organizer standardHalfVerticalMargin thickFont">نمونه محصولات</div>
                                        <div class="fourFullColumns">
                                            <div class="squareItem relativeMatter">
                                                <img src="" class="imageInMiddle">
                                            </div>
                                        </div>
                                        <div class="fourFullColumns">
                                            <div class="squareItem relativeMatter">
                                                <img src="" class="imageInMiddle">
                                            </div>
                                        </div>
                                        <div class="fourFullColumns ">
                                            <div class="squareItem relativeMatter">
                                                <img src="" class="imageInMiddle">
                                            </div>
                                        </div>
                                        <div class="fourFullColumns">
                                            <div class="squareItem relativeMatter">
                                                <img src="" class="imageInMiddle">
                                            </div>
                                        </div>
                                    </div>
                                    <div class="organizer twelveFontSize pinkColor">مشاهده همه محصولات</div>
                                </div>
                            </div>
                            <span class="goRight standardHalfVerticalMargin" ng-bind="Data[0].StoreName+' فروشگاه '">نام فروشگاه</span>
                        </div>
                        <div class="shopList loginPadding organizer">
                            <!-- be ezaye har mahsool yeki az in boxElement ha repeat mishe -->
                            <div class="boxElement radiusBox standardVerticalBottomMargin" ng-repeat="item in Data">
                                <div class="mainShopInfoes">
                                    <div class="shopContainer">
                                        <div class="mainInfo goRight listColumn">
                                            <div class="listHeader loginPadding borderBottom thickFont almostGrayColor">مشخصات اصلی</div>
                                            <div class="itemContainer thickFont organizer radiusBox standardVerticalBottomMargin relativeMatter loginPadding">
                                                <div class="organizer">
                                                    <div class="oneThreeFullColumn">
                                                        <div class="circleImageContainer">
                                                            <img ng-src="~/{{item.ImgAddress}}" alt="" class="imageInMiddle">
                                                        </div>
                                                    </div>
                                                    <div class="twoThreeFullColumn">
                                                        <div class="itemInfoes verticallyInCenter">
                                                            <div class="pinkColor" ng-bind="item.ProductName">نام محصول</div>
                                                            <div class="itemTitle greenColor organizer">iphone 6S 64GB</div>
                                                            <div class="itemDetail organizer smallExplain">دستگاه آیفون سون کارکرده با قیمت مناسب</div>
                                                            <div class="itemTags organizer">
                                                                <div class="tagInner outlineButton fillButton smallPadding whiteColor pinkBack">نام فروشگاه</div>
                                                                <div class="colorInner textInCenter standardHalfHorizontalMargin">
                                                                    <div class="boxElement radiusBox textInCenter relativeMatter">
                                                                        <span class="colorCircle objectInMiddle" style="background-color: #f00;"></span>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="shopPrice goRight listColumn rightBorder">
                                            <div class="listHeader  loginPadding borderBottom thickFont almostGrayColor">قیمت محصول</div>
                                            <div class="priceNotice loginPadding relativeMatter mixHeight">
                                                <div class="organizer objectInMiddle noWrap textInRight loginPadding">
                                                    <span class="offPrice lightGray crossOver standardHorizontalLeftMargin">390,000</span> <span class="mainPrice greenColor twentyTwoFontsize thickFont standardHorizontalLeftMargin">5,290,000 <span class="twelveFontSize">تومان</span></span>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="shopCount goRight listColumn rightBorder">
                                            <div class="listHeader  loginPadding borderBottom thickFont almostGrayColor">تعداد سفارش</div>
                                            <div class="counterContainer textInCenter loginPadding relativeMatter mixHeight">

                                                <div class="objectInMiddle">
                                                    <div class="organizer">
                                                        <span class="icon icon-arrow-up2 lightGray pointer" ng-click="CountPlus(item.ProductCode)"></span>
                                                    </div>
                                                    <div class="organizer">
                                                        <span class="changeableWidth eightyFontsize relativeMatter">
                                                            <span class="objectInMiddle" ng-bind="item.Count">1</span>
                                                        </span>
                                                    </div>
                                                    <div class="organizer aLittleFromBottom">
                                                        <span class="icon icon-arrow-down2  lightGray pointer" ng-click="CountMinus(item.ProductCode)"></span>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="shopTotalPrice goRight listColumn rightBorder">
                                            <div class="listHeader  loginPadding borderBottom thickFont almostGrayColor">قیمت کل</div>
                                            <div class="organizer textInRight loginPadding relativeMatter mixHeight">
                                                <div class="organizer objectInMiddle noWrap">
                                                    <span class="mainPrice redColor twentyTwoFontsize thickFont standardHorizontalLeftMargin loginPadding">5,290,000 <span class="twelveFontSize">تومان</span></span>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="deleteList goRight rightBorder relativeMatter lightRedBack pointer" ng-click="RemoveProduct(item.ProductCode)">
                                    <span class="icon icon-bin objectInMiddle twentyTwoFontsize redColor"></span>
                                </div>
                            </div>
                        </div>
                        <div class="bigCartWidth bigCartObjects stickToCenterLeft">
                            <div class="oneFiveColumn fullHeight stickToCenterRight">
                                <div class="plusSign organizer">
                                    <span class="icon icon-plus twentyTwoFontsize objectInMiddle  lightGray"></span>
                                </div>
                            </div>
                            <div class="fourFiveColumn greenCart boxElement radiusBox bigCartSpace standardDoublePadding">
                                <div class="cartContainer textInCenter thickFont greenColor">
                                    <div class="cartIcon">
                                        <img src="~/newAsset/images/truck.svg">
                                    </div>
                                    <div class="organizer cartTitle">هزینه ارسال</div>
                                    <div class="organizer cartPrice eightyFontsize">
                                        <span>28,000</span> <span class="twelveFontSize">تومان</span>
                                    </div>
                                    <div class="organizer cartExplain twelveFontSize darkGray">این محصول دارای هزینه ارسال می باشد</div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>

            @*تخفیف اگه داشته باشه*@
