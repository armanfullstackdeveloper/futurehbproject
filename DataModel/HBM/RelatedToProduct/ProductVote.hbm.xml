<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping assembly="DataModel" namespace="DataModel.Entities.RelatedToProduct" xmlns="urn:nhibernate-mapping-2.2">
  <class name="ProductVote" table="ProductVote" lazy="false" >
    <id name="Id" column="Id">
      <generator class="identity" />
    </id>
    <property name="ProductCode">
      <column name="ProductCode" sql-type="bigint" not-null="false" />
    </property>
    <property name="Ip">
      <column name="Ip" sql-type="nvarchar" not-null="false" />
    </property>
    <property name="Score">
      <column name="Score" sql-type="decimal" not-null="false" />
    </property>
  </class>
</hibernate-mapping>